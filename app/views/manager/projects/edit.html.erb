<div class="container bg-dark text-white rounded">
  <h1 class="display-4">Editing Project</h1>
</div>
<div class="d-flex">
      <div class="mr-auto p-2"><%= link_to 'Back', manager_projects_path, class: 'btn btn-secondary'%></div>
      <div class="p-2"><%= link_to 'Show', manager_project_path(@project), class: 'btn btn-info' %></div>
      <div class="p-2"><%= link_to 'Destroy', manager_project_path(@project),
      method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></div>
</div>
<hr>
<%= render 'form', project: @project, url: manager_project_path(@project) %>
